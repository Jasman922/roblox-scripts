    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").Events.Upgrades.upg4:FireServer(unpack(args))

local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").Events.Upgrades.upg5:FireServer(unpack(args))

local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").Events.Upgrades.upg6:FireServer(unpack(args))

local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").Events.Upgrades.upg7:FireServer(unpack(args))

local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").Events.Upgrades.upg8:FireServer(unpack(args))

local args = {
    [1] = game:GetService("Players").LocalPlayer
}

game:GetService("ReplicatedStorage").Events.Upgrades.upg9:FireServer(unpack(args))

